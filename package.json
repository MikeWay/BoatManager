{
  "name": "boat-manager",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build-all": "npm install && npm run build && npm run build-server",
    "build": "npm install && ng build",
    "build-server": "cd server && npm install && npm run build",
    "build-server-prod": "cd server && npm install && npm run build:prod",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "e2e": "npm run playwright test",
    "e2eui": "npx playwright test --ui",
    "gentest": "npx playwright codegen http://localhost:4200"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.1.1",
    "@angular/cdk": "^20.1.2",
    "@angular/common": "^20.1.1",
    "@angular/compiler": "^20.1.1",
    "@angular/core": "^20.1.1",
    "@angular/forms": "^20.1.1",
    "@angular/material": "^20.1.2",
    "@angular/platform-browser": "^20.1.1",
    "@angular/platform-browser-dynamic": "^20.1.1",
    "@angular/router": "^20.1.1",
    "@aws-sdk/lib-dynamodb": "^3.846.0",
    "@aws-sdk/util-dynamodb": "^3.846.0",
    "moment": "^2.30.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular/build": "^20.1.1",
    "@angular/cli": "^20.1.1",
    "@angular/compiler-cli": "^20.1.1",
    "@playwright/test": "^1.54.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^24.1.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.8.3"
  }
}
